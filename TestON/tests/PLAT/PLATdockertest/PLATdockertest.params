    </CASE110>

    <ENV>
        <cellName>productionCell</cellName>
        <cellApps>drivers,openflow,proxyarp,mobility</cellApps>
    </ENV>

    <GIT>
        <pull>False</pull>
        <branch>master</branch>
    </GIT>

    <CTRL>
        <port>6653</port>
    </CTRL>

    <SLEEP>
        <startup>60</startup>
    </SLEEP>

